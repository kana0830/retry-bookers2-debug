<% if @model == 'user' %>
  <h1>Search User</h1>
  <table class="table">
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <% @datas.each do |data| %>
      <tr>
        <td><%= attachment_image_tag @user, :profile_image, format: 'jpeg', fallback: "no-image-mini.jpg", size: "50x50" %></td>
        <td><%= data.name %></td>
        <td><%= link_to "Show", data, class: "user_#{data.id}" %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
<% end %>



